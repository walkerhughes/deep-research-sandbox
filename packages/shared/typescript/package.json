{
  "name": "@deep-research/shared",
  "version": "0.1.0",
  "description": "Shared types for Deep Research Agent",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "generate": "cd ../../../ && python scripts/generate_types.py",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "keywords": [
    "deep-research",
    "types",
    "shared"
  ],
  "license": "MIT"
}
